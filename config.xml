<?xml version='1.0' encoding='utf-8'?>
<widget id="org.anuta.kiss" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Simple GUI</name>
    <description>
	The Kiss flight controller configurator for Android. Based on original Felix/Flyduino code of the Chrome app.
    </description>
    <author email="fedor@anuta.org" href="http://www.anuta.org">
        Anuta Software
    </author>
    <content src="main.html" />
    <icon src="icon.png" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <access origin="http://ultraesc.de/" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>